{
  "name": "@heroku-cli/plugin-ps",
  "description": "ps core plugin for Heroku CLI",
  "version": "1.0.1",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/heroku/heroku-cli-plugin-ps/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^8.0.5",
    "@oclif/command": "^1.4.13",
    "@oclif/config": "^1.6.4",
    "cli-ux": "^3.3.31",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@oclif/dev-cli": "^1.13.1",
    "@oclif/plugin-help": "^1.2.4",
    "@oclif/test": "^1.0.5",
    "@oclif/tslint": "^1.1.0",
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.107",
    "@types/mocha": "^5.1.0",
    "@types/nock": "^9.1.3",
    "@types/node": "^9.6.5",
    "@types/supports-color": "^5.3.0",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.1.1",
    "nock": "^9.2.5",
    "ts-node": "6.0.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-plugin-ps",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-cli-plugin-ps",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && oclif-dev manifest && oclif-dev readme",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
