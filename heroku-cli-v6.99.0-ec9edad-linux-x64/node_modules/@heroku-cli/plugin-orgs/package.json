{
  "name": "@heroku-cli/plugin-orgs",
  "description": "CLI to manage Heroku Organizations",
  "version": "1.7.5",
  "author": "Raul Barroso @raulb",
  "bugs": {
    "url": "https://github.com/heroku/heroku-orgs/issues"
  },
  "dependencies": {
    "@heroku-cli/command": "^8.0.5",
    "co": "4.6.0",
    "heroku-cli-util": "^8.0.6",
    "inquirer": "^5.2.0",
    "lodash": "4.17.5",
    "lodash.flatten": "4.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.1",
    "@oclif/plugin-legacy": "^1.0.10",
    "chai": "^4.1.2",
    "mocha": "^5.1.0",
    "mocha-junit-reporter": "1.17.0",
    "nock": "^9.2.5",
    "nyc": "^11.6.0",
    "proxyquire": "^2.0.1",
    "standard": "^11.0.1"
  },
  "files": [
    ".oclif.manifest.json",
    "index.js",
    "commands",
    "lib"
  ],
  "homepage": "https://github.com/heroku/heroku-orgs",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/heroku-orgs"
  },
  "scripts": {
    "postpublish": "rm .oclif.manifest.json",
    "prepublishOnly": "oclif-dev manifest",
    "test": "nyc mocha && standard"
  },
  "cli-engine": {
    "topics": {
      "access": {
        "description": "manage user access to apps"
      },
      "orgs": {
        "description": "manage organizations"
      },
      "members": {
        "description": "manage organization members"
      },
      "teams": {
        "description": "manage teams"
      },
      "sharing": {
        "hidden": true
      },
      "join": {
        "hidden": true
      },
      "leave": {
        "hidden": true
      },
      "lock": {
        "hidden": true
      },
      "unlock": {
        "hidden": true
      }
    }
  }
}
